{
	"port": 8080,
	"environment": "DEVELOPMENT",
	"default_host": "http://host.docker.internal:8090/",
	"timeout": "3s",
	"cache_ttl": "3s",
	"output_encoding": "json",
	"extra_config": {
		"router": {
			"auto_options": true
		}
	},
	"client_tls": {
		"allow_insecure_connections": true
	},
	"plugin": {
		"pattern": ".so",
		"folder": "/etc/krakend/plugins"
	}
}
